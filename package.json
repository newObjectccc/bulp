  "license": "MIT",
  "dependencies": {
    "chalk": "~5.3.0",
    "cli-progress": "~3.12.0",
    "commander": "~11.1.0",
    "log-symbols": "~6.0.0",
    "ora": "~7.0.1",
    "prompts": "~2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/prompt-cli": "^18.4.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "commitlint": "^18.4.3",
